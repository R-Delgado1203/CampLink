    <!--generate divisions for each user in db-->
    {{#each users}}
    <div class="row">
        <div class="col-4">
            <img src="{{this.profile_pic}}" style="width: 130px; height: 130px;" />
        </div>
        <div class="col-4">
            <div class="row">
                <h2>{{this.first_name}} {{this.last_name}}</h2>
            </div>
            <div class="row">
                <a href="{{this.portfolio_link}}">Link to Portfolio</a>
            </div>
            <div class="row">
                <a href="{{this.github_link}}">Link to github</a>
            </div>
            <div class="row">
                <p>Favorite Languages:</p>
                <br>
                <p>{{this.languages}}</p>
            </div>            
        </div>
    </div>
    <hr>
    {{/each}}
